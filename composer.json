{
    "name": "Harmony/sail",
    "description": "Docker files for running a basic Harmony application.",
    "keywords": ["Harmony", "docker"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Harmony/sail/issues",
        "source": "https://github.com/Harmony/sail"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@Harmony.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "G4T/console": "^8.0|^9.0|^10.0",
        "G4T/contracts": "^8.0|^9.0|^10.0",
        "G4T/support": "^8.0|^9.0|^10.0",
        "symfony/yaml": "^6.0"
    },
    "bin": [
        "bin/sail"
    ],
    "autoload": {
        "psr-4": {
            "Harmony\\Sail\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "Harmony": {
            "providers": [
                "Harmony\\Sail\\SailServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpstan/phpstan": "^1.10"
    }
}
